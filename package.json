{
  "name": "baza-tooltips",
  "version": "0.1.0",
  "description": "",
  "main": "dist/Tooltips.js",
  "jsnext:main": "src/Tooltips.js",
  "module": "src/Tooltips.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "update:demo": "gulp update:demo",
    "watch:dist": "onchange \"dist/**/*.css\" \"dist/**/*.js\" -- npm run update:demo",
    "lint:js": "eslint -c .eslintrc.json --ignore-path .eslintignore --quiet src/*.js --ignore-pattern internals/scripts || exit 0",
    "prebuild:js": "npm run lint:js",
    "build:js": "rollup -c config/rollup.config.js",
    "watch:js": "onchange \"src/**/*.js\" -- npm run build:js",
    "build:styles": "gulp build:styles",
    "watch:styles": "gulp watch:styles",
    "serve": "browser-sync start --config config/bs.config.js",
    "start": "parallelshell -v -w \"npm run serve\" \"npm run watch:js\" \"npm run watch:styles\" \"npm run watch:dist\"",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bezet/baza-tooltips.git"
  },
  "author": "bezet",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.4.0",
    "browser-sync": "^2.18.11",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.0.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.4.1",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/bezet/baza-tooltips/issues"
  },
  "homepage": "https://github.com/bezet/baza-tooltips#readme"
}
