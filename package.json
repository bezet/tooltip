{
  "name": "baza-tooltips",
  "version": "0.1.0",
  "description": "A library for displaying tooltips.",
  "author": {
    "name": "bezet",
    "email": "bartosz.zadara@gmail.com",
    "url": "https://github.com/bezet"
  },
  "main": "dist/Tooltip.js",
  "jsnext:main": "src/Tooltip.js",
  "module": "src/Tooltip.js",
  "scripts": {
    "demo:update": "cp dist/* docs/dist",
    "dev:serve": "browser-sync start --config config/bs.config.js",
    "dev:build": "webpack --config config/webpack.dev.js --progress",
    "dev": "parallelshell -v -w \"npm run dev:serve\" \"npm run dev:build\"",
    "prod": "webpack --config config/webpack.prod.js --progress",
    "start": "npm run dev"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.4.0",
    "browser-sync": "^2.18.13",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "parallelshell": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "keywords": [
    "tooltips"
  ],
  "repository": "https://github.com/bezet/baza-tooltips",
  "license": "MIT"
}
