{
  "name": "baza-tooltips",
  "version": "0.1.0",
  "description": "",
  "main": "dist/Tooltips.js",
  "jsnext:main": "src/Tooltips.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "update:dist": "gulp update:dist",
    "update:demo": "gulp update:demo",
    "lint:js": "eslint -c .eslintrc.json --ignore-path .eslintignore --quiet src/*.js",
    "build:js": "npm run lint:js && rollup -c config/rollup.config.js",
    "build:css": "gulp build:css",
    "watch:scss": "gulp watch:scss",
    "watch:js": "onchange \"src/**/*.js\" -v -- npm run build:js",
    "serve": "browser-sync start --config config/bs-config.js",
    "start": "parallelshell -v -w \"npm run watch:scss\" \"npm run watch:js\" \"npm run serve\"",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bezet/baza-tooltips.git"
  },
  "author": "bezet",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-core": "^6.24.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-latest": "^6.24.0",
    "browser-sync": "^2.18.6",
    "chai": "^3.5.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "karma": "^1.7.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.3.0",
    "node-sass": "^4.2.0",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.6.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1"
  },
  "bugs": {
    "url": "https://github.com/bezet/baza-tooltips/issues"
  },
  "homepage": "https://github.com/bezet/baza-tooltips#readme"
